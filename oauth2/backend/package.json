{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist *.tsbuildinfo",
    "lint": "yarn prettier:check && yarn eslint",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn prettier:cli -- -l",
    "prettier:fix": "yarn prettier:cli -- --write",
    "eslint": "eslint .",
    "eslint:fix": "yarn eslint -- --fix",
    "pretest": "yarn clean && yarn build",
    "posttest": "yarn lint",
    "docker:build": "docker build -t backend .",
    "migrate": "node ./dist/migrate",
    "start": "yarn build && node -r source-map-support/register .",
    "prepare": "yarn build",
    "start:dev": "tsc && tsc-watch --onSuccess \"node -r source-map-support/register .\""
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.0.1",
    "@loopback/context": "^3.1.0",
    "@loopback/core": "^2.1.0",
    "@loopback/extension-logging": "^0.2.1",
    "@loopback/openapi-v3": "^3.1.1",
    "@loopback/repository": "^2.0.1",
    "@loopback/rest": "^3.0.1",
    "@loopback/rest-explorer": "^2.0.1",
    "@oryd/hydra-client": "^1.3.2",
    "dotenv": "^8.2.0",
    "loopback-connector-postgresql": "^3.9.1",
    "node-fetch": "^2.6.0",
    "openapi3-ts": "^1.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@loopback/cli": "^2.1.1",
    "@loopback/eslint-config": "^6.0.1",
    "@loopback/testlab": "^2.0.1",
    "@types/node": "^10.17.17",
    "@types/node-fetch": "^2.5.5",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.16",
    "tsc-watch": "^4.2.3",
    "typescript": "~3.8.3"
  }
}
