{
  "name": "exam-back",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t exam-backend .",
    "docker:run": "docker run -p 3000:3000 -d exam-backend",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "dev": "tsc && tsc-watch --onSuccess \"node -r source-map-support/register .\"",
    "gen": "ts-node openapi-generator/generate.ts"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "UNLICENSED",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.0.3",
    "@loopback/context": "^3.3.0",
    "@loopback/core": "^2.2.1",
    "@loopback/openapi-v3": "^3.1.2",
    "@loopback/repository": "^2.1.0",
    "@loopback/rest": "^3.2.0",
    "@loopback/rest-explorer": "^2.0.3",
    "@loopback/service-proxy": "^2.0.3",
    "@oryd/hydra-client": "^1.4.2",
    "@types/dockerode": "^2.5.28",
    "@types/ioredis": "^4.16.0",
    "@types/js-yaml": "^3.12.3",
    "@types/lodash": "^4.14.150",
    "@types/multer": "^1.4.3",
    "@types/node-fetch": "^2.5.5",
    "@types/socket.io": "^2.1.4",
    "dockerode": "^3.2.0",
    "dotenv": "^8.2.0",
    "execa": "^4.0.0",
    "express": "^4.17.1",
    "ioredis": "^4.16.3",
    "loopback-connector-kv-redis": "^3.0.3",
    "loopback-connector-postgresql": "^3.9.1",
    "loopback-connector-redis": "^3.0.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "redis": "^3.0.2",
    "socket.io": "^2.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.0.1",
    "@loopback/cli": "^2.3.0",
    "@loopback/eslint-config": "^6.0.3",
    "@loopback/testlab": "^3.0.0",
    "@openapitools/openapi-generator-cli": "^1.0.12-4.3.0",
    "@types/node": "^10.17.19",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "source-map-support": "^0.5.16",
    "tsc-watch": "^4.2.3",
    "typescript": "~3.8.3"
  }
}
